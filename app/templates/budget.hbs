<header>
  <h2 class="main__year">{{model.id}}</h2>
  {{#link-to "budget" previousId class="main__year-control"}}
    <i class="fa fa-chevron-left"></i>
  {{/link-to}}
  {{#link-to "budget" nextId class="main__year-control"}}
    <i class="fa fa-chevron-right"></i>
  {{/link-to}}
</header>

<table class="main__table budget-table">

  {{ops-head months}}
  {{ops-year model.months model.categories budget=model type="income"}}
  {{ops-year model.months model.categories budget=model type="outgo"}}

</table>
