{{! Totals }}
<tr>
  <td></td>
  {{#each months}}
    <th>0</th>
    <th>0</th>
  {{/each}}
</tr>

{{! Category operations }}
{{#each categories as |category|}}
  <tr>
    <th>{{category.name}}</th>
    {{#each months as |month|}}
      {{operation-by-month month category}}
    {{/each}}
  </tr>
{{/each}}
